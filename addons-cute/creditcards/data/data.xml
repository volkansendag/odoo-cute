<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

         <!-- Sequences for hospital.patient -->
        <record id="seq_creditcard" model="ir.sequence">
            <field name="name">CreditCard Sequence</field>
            <field name="code">creditcard</field>
            <field name="prefix">HP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_creditcard_bank" model="ir.sequence">
            <field name="name">CreditCard Bank Sequence</field>
            <field name="code">creditcard.bank</field>
            <field name="prefix">OT</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>